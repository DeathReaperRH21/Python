import numpy as np
A = np.array([np.sqrt(3)/2, -1, 0, 0, 0.5, 0, -1, 0, 1, 0, 0, -9.81, 0, 0, 1, 0]).reshape(4,4)
print('A =\n', A)
print('det(A) =', np.linalg.det(A))
b = np.array([0, 0, 0, 98.1]).reshape(4,1)
print('b =\n', b)
x = np.linalg.solve(A,b)
print('x =\n', x)
print('Compute A @ x =\n', A @ x)
print('Check if A @ x == b within tolerance 1e-6\n', abs(A@x - b) < 1e-6)
